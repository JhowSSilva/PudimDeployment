APP_NAME="Pudim Deployment"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
# Enhanced Features Configuration

# Notifications
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=

# Billing & Cost Management
BILLING_ENABLED=true
BILLING_CURRENCY=USD
BILLING_AUTO_INVOICE=true
BILLING_INVOICE_DAY=1

# AI & Machine Learning Features
AI_ENABLED=true
AI_MIN_DATA_POINTS=10
AI_PREDICTION_CONFIDENCE=0.8

# Security & Firewall
SECURITY_AUTO_BLOCK_THREATS=false
SECURITY_SCAN_INTERVAL=daily
FIREWALL_DEFAULT_POLICY=deny
FAIL2BAN_MAX_RETRY=5
FAIL2BAN_BAN_TIME=3600

# Performance & Caching
OPCACHE_ENABLED=true
REDIS_CACHE_ENABLED=true
MEMCACHED_ENABLED=false
BROTLI_COMPRESSION=true

# Application Performance Monitoring
APM_ENABLED=true
APM_SLOW_QUERY_THRESHOLD=1000
APM_TRACK_RESPONSE_TIME=true
APM_DETECT_N_PLUS_ONE=true

# Database Management
DB_AUTO_BACKUP=true
DB_BACKUP_RETENTION_DAYS=30
DB_BACKUP_SCHEDULE="0 2 * * *"

# Deployment Pipeline
DEPLOYMENT_AUTO_BACKUP=true
DEPLOYMENT_AUTO_ROLLBACK=true
DEPLOYMENT_HEALTH_CHECK_ENABLED=true
DEPLOYMENT_ZERO_DOWNTIME=true

# Queue Workers
QUEUE_WORKERS_AUTO_RESTART=true
QUEUE_WORKERS_MAX_MEMORY=512

# Monitoring & Alerts
MONITORING_ENABLED=true
ALERTS_CPU_THRESHOLD=80
ALERTS_MEMORY_THRESHOLD=85
ALERTS_DISK_THRESHOLD=90
# GitHub Integration
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI="${APP_URL}/github/callback"
GITHUB_WEBHOOK_SECRET=
